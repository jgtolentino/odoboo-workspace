# Odoo Production Environment Variables
# Copy to .env and fill in the values

# Database
DB_PASSWORD=CHANGE_ME_STRONG_PASSWORD_HERE
POSTGRES_DB=odoo
POSTGRES_USER=odoo

# Odoo
ODOO_TAG=18.0
ADMIN_PASSWORD=CHANGE_ME_ADMIN_PASSWORD_HERE

# OCR Integration
OCR_API_URL=https://ocr.insightpulseai.net/ocr
OCR_SECRET=PASTE_FROM_OCR_DROPLET_/etc/ocr/token

# Domain
ODOO_DOMAIN=odoo.insightpulseai.net
